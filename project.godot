; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="space impact"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://global/global.tscn"

[display]

window/size/viewport_width=176
window/size/viewport_height=99
window/size/resizable=false
window/size/window_width_override=640
window/size/window_height_override=360
window/stretch/mode="canvas_items"

[layer_names]

2d_physics/layer_1="obstacle"
2d_physics/layer_2="player"
2d_physics/layer_3="enemy"
2d_physics/layer_4="boss"
2d_physics/layer_5="bullets"
2d_physics/layer_8="wall"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
